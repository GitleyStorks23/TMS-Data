</span>
<span></span><span style="color: rgb(92, 99, 112); font-style: italic;"># Load the JSON file</span><span>
</span><span></span><span style="color: rgb(198, 120, 221);">with</span><span> </span><span style="color: rgb(230, 192, 123);">open</span><span>(</span><span style="color: rgb(152, 195, 121);">'concept_index.json'</span><span>, </span><span style="color: rgb(152, 195, 121);">'r'</span><span>) </span><span style="color: rgb(198, 120, 221);">as</span><span> file:
</span>    data = json.load(file)

<span></span><span style="color: rgb(92, 99, 112); font-style: italic;"># Extract vibrancy and flux from the concepts list</span><span>
</span><span></span><span class="hljs-function" style="color: rgb(198, 120, 221);">def</span><span class="hljs-function"> </span><span class="hljs-function" style="color: rgb(97, 174, 238);">extract_keys</span><span class="hljs-function">(</span><span class="hljs-function hljs-params">data</span><span class="hljs-function">):</span><span>
</span>    results = []
<span>    </span><span style="color: rgb(198, 120, 221);">if</span><span> </span><span style="color: rgb(152, 195, 121);">'concepts'</span><span> </span><span style="color: rgb(198, 120, 221);">in</span><span> data:
</span><span>        </span><span style="color: rgb(198, 120, 221);">for</span><span> item </span><span style="color: rgb(198, 120, 221);">in</span><span> data[</span><span style="color: rgb(152, 195, 121);">'concepts'</span><span>]:
</span><span>            </span><span style="color: rgb(198, 120, 221);">if</span><span> </span><span style="color: rgb(230, 192, 123);">isinstance</span><span>(item, </span><span style="color: rgb(230, 192, 123);">dict</span><span>):
</span><span>                vibrancy = item.get(</span><span style="color: rgb(152, 195, 121);">'vibrancy'</span><span>)
</span><span>                flux = item.get(</span><span style="color: rgb(152, 195, 121);">'flux'</span><span>)
</span><span>                </span><span style="color: rgb(198, 120, 221);">if</span><span> vibrancy </span><span style="color: rgb(198, 120, 221);">is</span><span> </span><span style="color: rgb(198, 120, 221);">not</span><span> </span><span style="color: rgb(86, 182, 194);">None</span><span> </span><span style="color: rgb(198, 120, 221);">or</span><span> flux </span><span style="color: rgb(198, 120, 221);">is</span><span> </span><span style="color: rgb(198, 120, 221);">not</span><span> </span><span style="color: rgb(86, 182, 194);">None</span><span>:
</span><span>                    results.append({</span><span style="color: rgb(152, 195, 121);">'vibrancy'</span><span>: vibrancy, </span><span style="color: rgb(152, 195, 121);">'flux'</span><span>: flux})
</span><span>    </span><span style="color: rgb(198, 120, 221);">return</span><span> results
</span>
<span></span><span style="color: rgb(92, 99, 112); font-style: italic;"># Process the data</span><span>
</span>extracted = extract_keys(data)

<span></span><span style="color: rgb(92, 99, 112); font-style: italic;"># Save to a new JSON file</span><span>
</span><span></span><span style="color: rgb(198, 120, 221);">with</span><span> </span><span style="color: rgb(230, 192, 123);">open</span><span>(</span><span style="color: rgb(152, 195, 121);">'extracted_data.json'</span><span>, </span><span style="color: rgb(152, 195, 121);">'w'</span><span>) </span><span style="color: rgb(198, 120, 221);">as</span><span> outfile:
</span><span>    json.dump(extracted, outfile, indent=</span><span style="color: rgb(209, 154, 102);">4</span><span>)
</span>
<span></span><span style="color: rgb(92, 99, 112); font-style: italic;"># Optional: Print results</span><span>
</span><span></span><span style="color: rgb(198, 120, 221);">for</span><span> result </span><span style="color: rgb(198, 120, 221);">in</span><span> extracted:
</span><span>    </span><span style="color: rgb(230, 192, 123);">print</span><span>(result)</span></code></div><div></div><div class="false flex flex-col gap-2 h-full"></div></div></div>

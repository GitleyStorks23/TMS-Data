</span>
<span></span><span style="color: rgb(92, 99, 112); font-style: italic;"># Function to create or get a branch with retry and sync</span><span>
</span><span></span><span class="hljs-function" style="color: rgb(198, 120, 221);">def</span><span class="hljs-function"> </span><span class="hljs-function" style="color: rgb(97, 174, 238);">create_or_get_branch</span><span class="hljs-function">(</span><span class="hljs-function hljs-params">branch_name</span><span class="hljs-function">):</span><span>
</span><span>    ref = </span><span style="color: rgb(152, 195, 121);">f"refs/heads/</span><span style="color: rgb(224, 108, 117);">{branch_name}</span><span style="color: rgb(152, 195, 121);">"</span><span>
</span><span>    max_retries = </span><span style="color: rgb(209, 154, 102);">3</span><span>
</span><span>    </span><span style="color: rgb(198, 120, 221);">for</span><span> attempt </span><span style="color: rgb(198, 120, 221);">in</span><span> </span><span style="color: rgb(230, 192, 123);">range</span><span>(max_retries):
</span><span>        </span><span style="color: rgb(198, 120, 221);">try</span><span>:
</span><span>            </span><span style="color: rgb(230, 192, 123);">print</span><span>(</span><span style="color: rgb(152, 195, 121);">f"Attempt </span><span style="color: rgb(224, 108, 117);">{attempt + </span><span style="color: rgb(209, 154, 102);">1</span><span style="color: rgb(224, 108, 117);">}</span><span style="color: rgb(152, 195, 121);">/</span><span style="color: rgb(224, 108, 117);">{max_retries}</span><span style="color: rgb(152, 195, 121);">: Checking for existing branch </span><span style="color: rgb(224, 108, 117);">{branch_name}</span><span style="color: rgb(152, 195, 121);">"</span><span>)
</span><span>            </span><span style="color: rgb(198, 120, 221);">return</span><span> repo.get_git_ref(ref)
</span><span>        </span><span style="color: rgb(198, 120, 221);">except</span><span> github.GithubException </span><span style="color: rgb(198, 120, 221);">as</span><span> e:
</span><span>            </span><span style="color: rgb(230, 192, 123);">print</span><span>(</span><span style="color: rgb(152, 195, 121);">f"Attempt </span><span style="color: rgb(224, 108, 117);">{attempt + </span><span style="color: rgb(209, 154, 102);">1</span><span style="color: rgb(224, 108, 117);">}</span><span style="color: rgb(152, 195, 121);">/</span><span style="color: rgb(224, 108, 117);">{max_retries}</span><span style="color: rgb(152, 195, 121);">: Branch </span><span style="color: rgb(224, 108, 117);">{branch_name}</span><span style="color: rgb(152, 195, 121);"> not found, attempting creation: </span><span style="color: rgb(224, 108, 117);">{</span><span style="color: rgb(230, 192, 123);">str</span><span style="color: rgb(224, 108, 117);">(e)}</span><span style="color: rgb(152, 195, 121);">"</span><span>)
</span><span>            </span><span style="color: rgb(198, 120, 221);">try</span><span>:
</span><span>                </span><span style="color: rgb(92, 99, 112); font-style: italic;"># Ensure main branch exists</span><span>
</span><span>                </span><span style="color: rgb(198, 120, 221);">try</span><span>:
</span><span>                    main_branch = repo.get_branch(</span><span style="color: rgb(152, 195, 121);">"main"</span><span>)
</span>                    sha = main_branch.commit.sha
<span>                </span><span style="color: rgb(198, 120, 221);">except</span><span> github.GithubException:
</span><span>                    </span><span style="color: rgb(230, 192, 123);">print</span><span>(</span><span style="color: rgb(152, 195, 121);">f"Attempt </span><span style="color: rgb(224, 108, 117);">{attempt + </span><span style="color: rgb(209, 154, 102);">1</span><span style="color: rgb(224, 108, 117);">}</span><span style="color: rgb(152, 195, 121);">/</span><span style="color: rgb(224, 108, 117);">{max_retries}</span><span style="color: rgb(152, 195, 121);">: Main branch not found, initializing"</span><span>)
</span><span>                    commits = </span><span style="color: rgb(230, 192, 123);">list</span><span>(repo.get_commits())
</span><span>                    </span><span style="color: rgb(198, 120, 221);">if</span><span> commits:
</span><span>                        sha = commits[</span><span style="color: rgb(209, 154, 102);">0</span><span>].sha
</span><span>                        repo.create_git_ref(ref=</span><span style="color: rgb(152, 195, 121);">"refs/heads/main"</span><span>, sha=sha)
</span><span>                        </span><span style="color: rgb(230, 192, 123);">print</span><span>(</span><span style="color: rgb(152, 195, 121);">f"Attempt </span><span style="color: rgb(224, 108, 117);">{attempt + </span><span style="color: rgb(209, 154, 102);">1</span><span style="color: rgb(224, 108, 117);">}</span><span style="color: rgb(152, 195, 121);">/</span><span style="color: rgb(224, 108, 117);">{max_retries}</span><span style="color: rgb(152, 195, 121);">: Created main branch with SHA </span><span style="color: rgb(224, 108, 117);">{sha}</span><span style="color: rgb(152, 195, 121);">"</span><span>)
</span><span>                    </span><span style="color: rgb(198, 120, 221);">else</span><span>:
</span><span>                        </span><span style="color: rgb(198, 120, 221);">raise</span><span> github.GithubException(</span><span style="color: rgb(209, 154, 102);">404</span><span>, </span><span style="color: rgb(152, 195, 121);">"No commits found to initialize main branch"</span><span>)
</span>                
<span>                </span><span style="color: rgb(92, 99, 112); font-style: italic;"># Create or handle User001</span><span>
</span><span>                </span><span style="color: rgb(198, 120, 221);">try</span><span>:
</span>                    repo.create_git_ref(ref=ref, sha=sha)
<span>                    </span><span style="color: rgb(230, 192, 123);">print</span><span>(</span><span style="color: rgb(152, 195, 121);">f"Attempt </span><span style="color: rgb(224, 108, 117);">{attempt + </span><span style="color: rgb(209, 154, 102);">1</span><span style="color: rgb(224, 108, 117);">}</span><span style="color: rgb(152, 195, 121);">/</span><span style="color: rgb(224, 108, 117);">{max_retries}</span><span style="color: rgb(152, 195, 121);">: Successfully created branch </span><span style="color: rgb(224, 108, 117);">{branch_name}</span><span style="color: rgb(152, 195, 121);">"</span><span>)
</span><span>                    time.sleep(</span><span style="color: rgb(209, 154, 102);">2</span><span>)  </span><span style="color: rgb(92, 99, 112); font-style: italic;"># Wait for sync</span><span>
</span><span>                    </span><span style="color: rgb(92, 99, 112); font-style: italic;"># Wait for branch to be accessible</span><span>
</span><span>                    </span><span style="color: rgb(198, 120, 221);">for</span><span> i </span><span style="color: rgb(198, 120, 221);">in</span><span> </span><span style="color: rgb(230, 192, 123);">range</span><span>(</span><span style="color: rgb(209, 154, 102);">5</span><span>):
</span><span>                        </span><span style="color: rgb(198, 120, 221);">try</span><span>:
</span><span>                            </span><span style="color: rgb(198, 120, 221);">return</span><span> repo.get_git_ref(ref)
</span><span>                        </span><span style="color: rgb(198, 120, 221);">except</span><span> github.GithubException:
</span><span>                            </span><span style="color: rgb(230, 192, 123);">print</span><span>(</span><span style="color: rgb(152, 195, 121);">f"Attempt </span><span style="color: rgb(224, 108, 117);">{attempt + </span><span style="color: rgb(209, 154, 102);">1</span><span style="color: rgb(224, 108, 117);">}</span><span style="color: rgb(152, 195, 121);">/</span><span style="color: rgb(224, 108, 117);">{max_retries}</span><span style="color: rgb(152, 195, 121);">: Waiting for </span><span style="color: rgb(224, 108, 117);">{branch_name}</span><span style="color: rgb(152, 195, 121);"> to sync, try </span><span style="color: rgb(224, 108, 117);">{i + </span><span style="color: rgb(209, 154, 102);">1</span><span style="color: rgb(224, 108, 117);">}</span><span style="color: rgb(152, 195, 121);">/5"</span><span>)
</span><span>                            time.sleep(</span><span style="color: rgb(209, 154, 102);">1</span><span>)
</span><span>                    </span><span style="color: rgb(198, 120, 221);">raise</span><span> github.GithubException(</span><span style="color: rgb(209, 154, 102);">404</span><span>, </span><span style="color: rgb(152, 195, 121);">f"Failed to sync </span><span style="color: rgb(224, 108, 117);">{branch_name}</span><span style="color: rgb(152, 195, 121);"> after creation"</span><span>)
</span><span>                </span><span style="color: rgb(198, 120, 221);">except</span><span> github.GithubException </span><span style="color: rgb(198, 120, 221);">as</span><span> e:
</span><span>                    </span><span style="color: rgb(198, 120, 221);">if</span><span> e.status == </span><span style="color: rgb(209, 154, 102);">422</span><span> </span><span style="color: rgb(198, 120, 221);">and</span><span> </span><span style="color: rgb(152, 195, 121);">"already exists"</span><span> </span><span style="color: rgb(198, 120, 221);">in</span><span> </span><span style="color: rgb(230, 192, 123);">str</span><span>(e):
</span><span>                        </span><span style="color: rgb(230, 192, 123);">print</span><span>(</span><span style="color: rgb(152, 195, 121);">f"Attempt </span><span style="color: rgb(224, 108, 117);">{attempt + </span><span style="color: rgb(209, 154, 102);">1</span><span style="color: rgb(224, 108, 117);">}</span><span style="color: rgb(152, 195, 121);">/</span><span style="color: rgb(224, 108, 117);">{max_retries}</span><span style="color: rgb(152, 195, 121);">: Branch </span><span style="color: rgb(224, 108, 117);">{branch_name}</span><span style="color: rgb(152, 195, 121);"> already exists, retrieving it"</span><span>)
</span><span>                        time.sleep(</span><span style="color: rgb(209, 154, 102);">1</span><span>)  </span><span style="color: rgb(92, 99, 112); font-style: italic;"># Wait for sync</span><span>
</span><span>                        </span><span style="color: rgb(198, 120, 221);">return</span><span> repo.get_git_ref(ref)
</span><span>                    </span><span style="color: rgb(198, 120, 221);">else</span><span>:
</span><span>                        </span><span style="color: rgb(198, 120, 221);">raise</span><span> e
</span><span>            </span><span style="color: rgb(198, 120, 221);">except</span><span> github.GithubException </span><span style="color: rgb(198, 120, 221);">as</span><span> e:
</span><span>                </span><span style="color: rgb(230, 192, 123);">print</span><span>(</span><span style="color: rgb(152, 195, 121);">f"Attempt </span><span style="color: rgb(224, 108, 117);">{attempt + </span><span style="color: rgb(209, 154, 102);">1</span><span style="color: rgb(224, 108, 117);">}</span><span style="color: rgb(152, 195, 121);">/</span><span style="color: rgb(224, 108, 117);">{max_retries}</span><span style="color: rgb(152, 195, 121);">: Failed to resolve branch setup: </span><span style="color: rgb(224, 108, 117);">{</span><span style="color: rgb(230, 192, 123);">str</span><span style="color: rgb(224, 108, 117);">(e)}</span><span style="color: rgb(152, 195, 121);">"</span><span>)
</span><span>                </span><span style="color: rgb(198, 120, 221);">if</span><span> attempt &lt; max_retries - </span><span style="color: rgb(209, 154, 102);">1</span><span>:
</span><span>                    time.sleep(</span><span style="color: rgb(209, 154, 102);">2</span><span> ** attempt)  </span><span style="color: rgb(92, 99, 112); font-style: italic;"># Exponential backoff</span><span>
</span><span>                    </span><span style="color: rgb(198, 120, 221);">continue</span><span>
</span><span>                </span><span style="color: rgb(198, 120, 221);">raise</span><span> e
</span>
<span></span><span style="color: rgb(92, 99, 112); font-style: italic;"># Function to react to 10-key updates</span><span>
</span><span></span><span class="hljs-function" style="color: rgb(198, 120, 221);">def</span><span class="hljs-function"> </span><span class="hljs-function" style="color: rgb(97, 174, 238);">react_to_keys</span><span class="hljs-function">(</span><span class="hljs-function hljs-params">node_path, keys</span><span class="hljs-function">):</span><span>
</span>    updated_keys = keys.copy()
<span>    </span><span style="color: rgb(198, 120, 221);">for</span><span> key, value </span><span style="color: rgb(198, 120, 221);">in</span><span> keys.items():
</span><span>        </span><span style="color: rgb(198, 120, 221);">if</span><span> key == </span><span style="color: rgb(152, 195, 121);">"vibrancy"</span><span> </span><span style="color: rgb(198, 120, 221);">and</span><span> value &gt; </span><span style="color: rgb(209, 154, 102);">0.8</span><span>:
</span><span>            updated_keys[key] *= </span><span style="color: rgb(209, 154, 102);">1.2</span><span>  </span><span style="color: rgb(92, 99, 112); font-style: italic;"># Amplify high vibrancy</span><span>
</span><span>        </span><span style="color: rgb(198, 120, 221);">elif</span><span> key == </span><span style="color: rgb(152, 195, 121);">"flux"</span><span> </span><span style="color: rgb(198, 120, 221);">and</span><span> value &lt; </span><span style="color: rgb(209, 154, 102);">0.3</span><span>:
</span><span>            updated_keys[key] *= </span><span style="color: rgb(209, 154, 102);">0.8</span><span>  </span><span style="color: rgb(92, 99, 112); font-style: italic;"># Dampen low flux</span><span>
</span><span>        </span><span style="color: rgb(198, 120, 221);">if</span><span> </span><span style="color: rgb(152, 195, 121);">"divergence"</span><span> </span><span style="color: rgb(198, 120, 221);">in</span><span> keys </span><span style="color: rgb(198, 120, 221);">and</span><span> keys[</span><span style="color: rgb(152, 195, 121);">"divergence"</span><span>] &gt; </span><span style="color: rgb(209, 154, 102);">0.8</span><span>:
</span><span>            updated_keys[</span><span style="color: rgb(152, 195, 121);">"new_subnode"</span><span>] = </span><span style="color: rgb(86, 182, 194);">True</span><span>  </span><span style="color: rgb(92, 99, 112); font-style: italic;"># Trigger sub-node</span><span>
</span><span>    </span><span style="color: rgb(198, 120, 221);">return</span><span> updated_keys
</span>
<span></span><span style="color: rgb(92, 99, 112); font-style: italic;"># Function to commit node data with link chain updates</span><span>
</span><span></span><span class="hljs-function" style="color: rgb(198, 120, 221);">def</span><span class="hljs-function"> </span><span class="hljs-function" style="color: rgb(97, 174, 238);">commit_node_data</span><span class="hljs-function">(</span><span class="hljs-function hljs-params">branch_name, category, subcategory, concept, keys</span><span class="hljs-function">):</span><span>
</span><span>    timestamp = </span><span style="color: rgb(230, 192, 123);">int</span><span>(time.time())
</span><span>    node_path = </span><span style="color: rgb(152, 195, 121);">f"</span><span style="color: rgb(224, 108, 117);">{category}</span><span style="color: rgb(152, 195, 121);">/</span><span style="color: rgb(224, 108, 117);">{subcategory}</span><span style="color: rgb(152, 195, 121);">/</span><span style="color: rgb(224, 108, 117);">{concept}</span><span style="color: rgb(152, 195, 121);">_</span><span style="color: rgb(224, 108, 117);">{timestamp}</span><span style="color: rgb(152, 195, 121);">.png"</span><span>
</span>    metadata = {
<span>        </span><span style="color: rgb(152, 195, 121);">"concept"</span><span>: concept,
</span><span>        </span><span style="color: rgb(152, 195, 121);">"category"</span><span>: category,
</span><span>        </span><span style="color: rgb(152, 195, 121);">"subcategory"</span><span>: subcategory,
</span><span>        </span><span style="color: rgb(152, 195, 121);">"10_keys"</span><span>: keys,
</span><span>        </span><span style="color: rgb(152, 195, 121);">"timestamp"</span><span>: timestamp,
</span><span>        </span><span style="color: rgb(152, 195, 121);">"hash"</span><span>: base64.b64encode(</span><span style="color: rgb(230, 192, 123);">str</span><span>(keys).encode()).decode(</span><span style="color: rgb(152, 195, 121);">'utf-8'</span><span>)  </span><span style="color: rgb(92, 99, 112); font-style: italic;"># Crypto hash</span><span>
</span>    }

<span>    </span><span style="color: rgb(92, 99, 112); font-style: italic;"># Simulate pixel data (replace with generate_matrix.py output)</span><span>
</span><span>    pixel_data = base64.b64encode(</span><span style="color: rgb(152, 195, 121);">b"Simulated pixel data"</span><span>).decode(</span><span style="color: rgb(152, 195, 121);">'utf-8'</span><span>)
</span>    repo.create_file(
        path=node_path,
<span>        message=</span><span style="color: rgb(152, 195, 121);">f"Add </span><span style="color: rgb(224, 108, 117);">{concept}</span><span style="color: rgb(152, 195, 121);"> node at </span><span style="color: rgb(224, 108, 117);">{timestamp}</span><span style="color: rgb(152, 195, 121);">"</span><span>,
</span>        content=pixel_data,
        branch=branch_name
    )
    repo.create_file(
<span>        path=</span><span style="color: rgb(152, 195, 121);">f"metadata/</span><span style="color: rgb(224, 108, 117);">{concept}</span><span style="color: rgb(152, 195, 121);">_</span><span style="color: rgb(224, 108, 117);">{timestamp}</span><span style="color: rgb(152, 195, 121);">.json"</span><span>,
</span><span>        message=</span><span style="color: rgb(152, 195, 121);">f"Add metadata for </span><span style="color: rgb(224, 108, 117);">{concept}</span><span style="color: rgb(152, 195, 121);"> at </span><span style="color: rgb(224, 108, 117);">{timestamp}</span><span style="color: rgb(152, 195, 121);">"</span><span>,
</span>        content=json.dumps(metadata),
        branch=branch_name
    )
<span>    </span><span style="color: rgb(230, 192, 123);">print</span><span>(</span><span style="color: rgb(152, 195, 121);">f"Committed </span><span style="color: rgb(224, 108, 117);">{concept}</span><span style="color: rgb(152, 195, 121);"> to </span><span style="color: rgb(224, 108, 117);">{node_path}</span><span style="color: rgb(152, 195, 121);">"</span><span>)
</span>
<span></span><span style="color: rgb(92, 99, 112); font-style: italic;"># Function to follow links and update chain</span><span>
</span><span></span><span class="hljs-function" style="color: rgb(198, 120, 221);">def</span><span class="hljs-function"> </span><span class="hljs-function" style="color: rgb(97, 174, 238);">follow_links_and_update</span><span class="hljs-function">(</span><span class="hljs-function hljs-params">index_url, use_link_chains=</span><span class="hljs-function hljs-params" style="color: rgb(86, 182, 194);">True</span><span class="hljs-function">):</span><span>
</span><span>    </span><span style="color: rgb(198, 120, 221);">if</span><span> </span><span style="color: rgb(198, 120, 221);">not</span><span> check_rate_limit():
</span><span>        </span><span style="color: rgb(230, 192, 123);">print</span><span>(</span><span style="color: rgb(152, 195, 121);">"Rate limit too low, aborting update."</span><span>)
</span><span>        </span><span style="color: rgb(198, 120, 221);">return</span><span>
</span><span>    headers = {</span><span style="color: rgb(152, 195, 121);">"Authorization"</span><span>: </span><span style="color: rgb(152, 195, 121);">f"token </span><span style="color: rgb(224, 108, 117);">{TOKEN}</span><span style="color: rgb(152, 195, 121);">"</span><span>}
</span><span>    response = requests.get(index_url, headers=headers)  </span><span style="color: rgb(92, 99, 112); font-style: italic;"># Use token in headers for private access</span><span>
</span><span>    </span><span style="color: rgb(198, 120, 221);">if</span><span> response.status_code == </span><span style="color: rgb(209, 154, 102);">200</span><span>:
</span>        index_data = response.json()
<span>        branch_name = </span><span style="color: rgb(152, 195, 121);">"User001"</span><span>
</span>        create_or_get_branch(branch_name)
<span>        </span><span style="color: rgb(198, 120, 221);">for</span><span> category, subcats </span><span style="color: rgb(198, 120, 221);">in</span><span> index_data.items():
</span><span>            </span><span style="color: rgb(198, 120, 221);">for</span><span> subcategory, concepts </span><span style="color: rgb(198, 120, 221);">in</span><span> subcats.items():
</span><span>                </span><span style="color: rgb(198, 120, 221);">for</span><span> concept, link </span><span style="color: rgb(198, 120, 221);">in</span><span> concepts.items():
</span><span>                    </span><span style="color: rgb(198, 120, 221);">try</span><span>:
</span>                        sub_response = requests.get(link, headers=headers)
<span>                        </span><span style="color: rgb(198, 120, 221);">if</span><span> sub_response.status_code == </span><span style="color: rgb(209, 154, 102);">200</span><span>:
</span><span>                            </span><span style="color: rgb(92, 99, 112); font-style: italic;"># Simulate 10-key valuation based on context (e.g., tone)</span><span>
</span>                            keys = {
<span>                                </span><span style="color: rgb(152, 195, 121);">"resonance"</span><span>: </span><span style="color: rgb(209, 154, 102);">0.5</span><span>, </span><span style="color: rgb(152, 195, 121);">"vibrancy"</span><span>: </span><span style="color: rgb(209, 154, 102);">0.7</span><span>, </span><span style="color: rgb(152, 195, 121);">"coherence"</span><span>: </span><span style="color: rgb(209, 154, 102);">0.6</span><span>,
</span><span>                                </span><span style="color: rgb(152, 195, 121);">"flux"</span><span>: </span><span style="color: rgb(209, 154, 102);">0.4</span><span>, </span><span style="color: rgb(152, 195, 121);">"intensity"</span><span>: </span><span style="color: rgb(209, 154, 102);">0.5</span><span>, </span><span style="color: rgb(152, 195, 121);">"harmony"</span><span>: </span><span style="color: rgb(209, 154, 102);">0.6</span><span>,
</span><span>                                </span><span style="color: rgb(152, 195, 121);">"divergence"</span><span>: </span><span style="color: rgb(209, 154, 102);">0.7</span><span>, </span><span style="color: rgb(152, 195, 121);">"persistence"</span><span>: </span><span style="color: rgb(209, 154, 102);">0.5</span><span>, </span><span style="color: rgb(152, 195, 121);">"amplitude"</span><span>: </span><span style="color: rgb(209, 154, 102);">0.6</span><span>,
</span><span>                                </span><span style="color: rgb(152, 195, 121);">"continuity"</span><span>: </span><span style="color: rgb(209, 154, 102);">0.4</span><span>
</span>                            }
<span>                            updated_keys = react_to_keys(</span><span style="color: rgb(152, 195, 121);">f"</span><span style="color: rgb(224, 108, 117);">{category}</span><span style="color: rgb(152, 195, 121);">/</span><span style="color: rgb(224, 108, 117);">{subcategory}</span><span style="color: rgb(152, 195, 121);">/</span><span style="color: rgb(224, 108, 117);">{concept}</span><span style="color: rgb(152, 195, 121);">"</span><span>, keys)
</span>                            commit_node_data(branch_name, category, subcategory, concept, updated_keys)
<span>                            </span><span style="color: rgb(92, 99, 112); font-style: italic;"># Strengthen parent nodes (link chain)</span><span>
</span><span>                            </span><span style="color: rgb(198, 120, 221);">if</span><span> use_link_chains:
</span><span>                                </span><span style="color: rgb(198, 120, 221);">for</span><span> parent </span><span style="color: rgb(198, 120, 221);">in</span><span> [category, subcategory]:
</span><span>                                    parent_keys = {</span><span style="color: rgb(152, 195, 121);">"vibrancy"</span><span>: </span><span style="color: rgb(209, 154, 102);">0.1</span><span>}  </span><span style="color: rgb(92, 99, 112); font-style: italic;"># Incremental boost</span><span>
</span><span>                                    commit_node_data(branch_name, parent, </span><span style="color: rgb(152, 195, 121);">""</span><span>, parent, parent_keys)
</span><span>                            </span><span style="color: rgb(230, 192, 123);">print</span><span>(</span><span style="color: rgb(152, 195, 121);">f"Followed chain for </span><span style="color: rgb(224, 108, 117);">{concept}</span><span style="color: rgb(152, 195, 121);">, updated </span><span style="color: rgb(224, 108, 117);">{category}</span><span style="color: rgb(152, 195, 121);">/</span><span style="color: rgb(224, 108, 117);">{subcategory}</span><span style="color: rgb(152, 195, 121);">"</span><span>)
</span><span>                        </span><span style="color: rgb(198, 120, 221);">else</span><span>:
</span><span>                            </span><span style="color: rgb(230, 192, 123);">print</span><span>(</span><span style="color: rgb(152, 195, 121);">f"Failed to access </span><span style="color: rgb(224, 108, 117);">{link}</span><span style="color: rgb(152, 195, 121);">: </span><span style="color: rgb(224, 108, 117);">{sub_response.status_code}</span><span style="color: rgb(152, 195, 121);">"</span><span>)
</span><span>                    </span><span style="color: rgb(198, 120, 221);">except</span><span> requests.RequestException </span><span style="color: rgb(198, 120, 221);">as</span><span> e:
</span><span>                        </span><span style="color: rgb(230, 192, 123);">print</span><span>(</span><span style="color: rgb(152, 195, 121);">f"Skipped </span><span style="color: rgb(224, 108, 117);">{link}</span><span style="color: rgb(152, 195, 121);"> due to error: </span><span style="color: rgb(224, 108, 117);">{e}</span><span style="color: rgb(152, 195, 121);">"</span><span>)
</span><span>                        </span><span style="color: rgb(198, 120, 221);">continue</span><span>
</span><span>    </span><span style="color: rgb(198, 120, 221);">else</span><span>:
</span><span>        </span><span style="color: rgb(230, 192, 123);">print</span><span>(</span><span style="color: rgb(152, 195, 121);">f"Failed to access index </span><span style="color: rgb(224, 108, 117);">{index_url}</span><span style="color: rgb(152, 195, 121);">: </span><span style="color: rgb(224, 108, 117);">{response.status_code}</span><span style="color: rgb(152, 195, 121);"> - Ensure token matches and has repo access"</span><span>)
</span>
<span></span><span style="color: rgb(92, 99, 112); font-style: italic;"># Main execution</span><span>
</span><span></span><span style="color: rgb(198, 120, 221);">if</span><span> __name__ == </span><span style="color: rgb(152, 195, 121);">"__main__"</span><span>:
</span><span>    index_url = </span><span style="color: rgb(152, 195, 121);">"https://raw.githubusercontent.com/GitleyStorks23/Vision/refs/heads/User001/concept_index.json?token=GHSAT0AAAAAADFMLDQJS24KDLWQATFHWYAW2CHYANA"</span><span>
</span><span>    follow_links_and_update(index_url, use_link_chains=</span><span style="color: rgb(86, 182, 194);">True</span><span>)</span></code></div><div></div><div class="false flex flex-col gap-2 h-full"></div></div></div>

<span></span><span style="color: rgb(92, 99, 112); font-style: italic;"># GitHub authentication</span><span>
</span><span></span><span style="color: rgb(198, 120, 221);">with</span><span> </span><span style="color: rgb(230, 192, 123);">open</span><span>(</span><span style="color: rgb(152, 195, 121);">"api_token.txt"</span><span>, </span><span style="color: rgb(152, 195, 121);">"r"</span><span>) </span><span style="color: rgb(198, 120, 221);">as</span><span> f:
</span>    TOKEN = f.read().strip()
g = Github(TOKEN)
<span>repo = g.get_repo(</span><span style="color: rgb(152, 195, 121);">"GitleyStorks23/Vision"</span><span>)  </span><span style="color: rgb(92, 99, 112); font-style: italic;"># "Vision" repository</span><span>
